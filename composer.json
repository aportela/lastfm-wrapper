{
    "name": "aportela/lastfm-wrapper",
    "description": "Custom last.fm api wrapper",
    "type": "library",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "alex",
            "email": "766f6964+github@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
